    "jwt-decode": "4.0.0",
    "keycloak-js": "25.0.6",
    "mantine-datatable": "8.2.0",
    "react": "19.1.1",
    "react-avatar-editor": "14.0.0-beta.6",
    "react-dom": "19.1.1",
    "react-router": "7.8.2"
